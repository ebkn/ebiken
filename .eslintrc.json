{
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "plugins": [
    "import",
    "@typescript-eslint",
    "react",
    "jsx-a11y"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }]
  }
}
